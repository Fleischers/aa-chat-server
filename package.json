{
  "name": "aa-chat-server",
  "version": "0.0.1",
  "description": "Websocket chat server module, purposed to attach to another server",
  "main": "app.js",
  "scripts": {
    "start": "./node_modules/.bin/forever --minUptime 1000 --spinSleepTime 1000 app.js",
    "test": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "mocha": "./node_modules/.bin/mocha test",
    "precommit_install": "./node_modules/precommit/bin/cli.js install",
    "precommit_remove": "./node_modules/precommit/bin/cli.js remove",
    "unstaged": "git status --porcelain",
    "codeclimate": "CODECLIMATE_REPO_TOKEN= codeclimate < ./coverage/lcov.info",
    "coverage": "open coverage/lcov-report/index.html",
    "eslint": "./node_modules/.bin/eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fleischers/aa-chat-server.git"
  },
  "keywords": [
    "chat",
    "websockets"
  ],
  "author": "Glib Ischenko <fcl@gorlifsense.com> (http://fleischer.gorlifsense.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fleischers/aa-chat-server/issues"
  },
  "homepage": "https://github.com/Fleischers/aa-chat-server#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.3.1",
    "eslint": "^2.7.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "pre-commit": "^1.1.2",
    "precommit": "^1.2.0",
    "socket.io-client": "^1.4.5"
  },
  "dependencies": {
    "forever": "^0.15.1",
    "socket.io": "^1.4.5"
  },
  "pre-commit": [
    "eslint",
    "test",
    "unstaged"
  ]
}
